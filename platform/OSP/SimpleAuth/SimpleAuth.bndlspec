<?xml version="1.0"?>
<bundlespec>
  <manifest>
    <name>Simple Authentication Service for OSP</name>
    <symbolicName>com.appinf.osp.simple-auth</symbolicName>
    <version>1.0.0</version>
    <vendor>Applied Informatics</vendor>
    <copyright>(c) 2007-2017, Applied Informatics Software Engineering GmbH</copyright>
    <activator>
      <class>SimpleAuthBundleActivator</class>
      <library>com.appinf.osp.simple-auth</library>
    </activator>
    <lazyStart>false</lazyStart>
    <runLevel>000</runLevel>
  </manifest>
  <code>
    ${bin}/*.dll,
    ${bin}/*.pdb,
    bin/${osName}/${osArch}/*.so,
    bin/${osName}/${osArch}/*.dylib
  </code>
  <files>
    bundle/*
  </files>
</bundlespec>
