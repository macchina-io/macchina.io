<AppConfig>
	<RemoteGen>
		<files>
			<include>
				${POCO_BASE}/RemotingNG/include/Poco/RemotingNG/RemoteObject.h
				${POCO_BASE}/RemotingNG/include/Poco/RemotingNG/Proxy.h
				${POCO_BASE}/RemotingNG/include/Poco/RemotingNG/Skeleton.h
				include/TimeService.h
			</include>
			<exclude>
			</exclude>
		</files>
		<output>
			<include>include</include>
			<src>src</src>
			<schema>wsdl</schema>
			<copyright>Copyright (c) 2006-2016, Applied Informatics Software Engineering GmbH.</copyright>
			<mode>server</mode>
			<timestamps>false</timestamps>
		</output>
		<schema>
			<TimeService>
				<serviceLocation>http://localhost:8080/soap/TimeService/TheClock</serviceLocation>
			</TimeService>
		</schema>
		<compiler id="gcc">
			<exec>g++</exec>
			<options>
				-I${POCO_BASE}/Foundation/include
				-I${POCO_BASE}/RemotingNG/include
				-I./include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
		<compiler id="clang">
			<exec>clang++</exec>
			<options>
				-I${POCO_BASE}/Foundation/include
				-I${POCO_BASE}/RemotingNG/include
				-I./include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-xc++
				-o%.i
			</options>
		</compiler>
		<compiler id="msvc">
			<exec>cl</exec>
			<options>
				/I 
				"${POCO_BASE}\Foundation\include"
				/I 
				"${POCO_BASE}\RemotingNG\include"
				/I 
				".\include"
				/nologo
				/C
				/P
				/TP
			</options>
		</compiler>
	</RemoteGen>
</AppConfig>
