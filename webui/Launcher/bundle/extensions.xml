<extensions>
  <extension point="osp.web.server.requesthandler" path="/" class="IoT::Web::Launcher::ControllerRequestHandlerFactory" library="io.macchina.webui.launcher" allowSpecialization="all" hidden="true"/>
  <extension point="osp.web.server.requesthandler" path="/macchina/login" class="IoT::Web::Launcher::LoginPageFactory" library="io.macchina.webui.launcher" allowSpecialization="none" hidden="true"/>
  <extension point="osp.web.server.requesthandler" path="/macchina/session.json" class="IoT::Web::Launcher::SessionJSONFactory" library="io.macchina.webui.launcher" allowSpecialization="none" hidden="true"/>

  <extension point="osp.web.server.directory" path="/macchina/launcher" resource="launcher" allowSpecialization="owner" hidden="true"/>
  <extension point="osp.web.server.requesthandler" path="/macchina/launcher/apps.json" class="IoT::Web::Launcher::LauncherAppsJSONFactory" library="io.macchina.webui.launcher" allowSpecialization="owner" hidden="true"/>

  <extension point="osp.web.server.directory" path="/css" resource="css" allowSpecialization="none" hidden="true"/>
  <extension point="osp.web.server.directory" path="/images" resource="images" allowSpecialization="none" hidden="true"/>
  <extension point="osp.web.server.directory" path="/jquery" resource="jquery" allowSpecialization="none" hidden="true"/>
  <extension point="osp.web.server.directory" path="/angular" resource="angular" allowSpecialization="none" hidden="true"/>
  <extension point="osp.web.server.directory" path="/fonts" resource="fonts" allowSpecialization="none" hidden="true"/>
</extensions>
