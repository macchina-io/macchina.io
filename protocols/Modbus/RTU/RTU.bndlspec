<bundlespec>
	<manifest>
    	<name>macchina.io Modbus RTU</name>
		<symbolicName>io.macchina.modbus.rtu</symbolicName>
		<version>1.0.0</version>
		<vendor>Applied Informatics</vendor>
		<copyright>(c) 2015-2022, Applied Informatics Software Engineering GmbH</copyright>
		<activator>
			<class>IoT::Modbus::RTU::BundleActivator</class>
			<library>io.macchina.modbus.rtu</library>
		</activator>
		<dependency>
			<symbolicName>io.macchina.modbus.core</symbolicName>
			<version>[1.0.0, 2.0.0)</version>
		</dependency>
		<dependency>
			<symbolicName>io.macchina.serial</symbolicName>
			<version>[1.0.0, 2.0.0)</version>
		</dependency>
		<lazyStart>false</lazyStart>
		<runLevel>610</runLevel>
	</manifest>
	<code>
		bin/${osName}/${osArch}/*.so,
		bin/${osName}/${osArch}/*.dylib,
	</code>
	<files>
		bundle/*
	</files>
</bundlespec>
