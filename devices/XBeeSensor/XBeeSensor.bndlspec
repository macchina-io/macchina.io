<bundlespec>
	<manifest>
    	<name>macchina.io XBee Sensor</name>
		<symbolicName>io.macchina.xbee.sensor</symbolicName>
		<version>1.0.0</version>
		<vendor>Applied Informatics</vendor>
		<copyright>(c) 2015, Applied Informatics Software Engineering GmbH</copyright>
		<activator>
			<class>IoT::XBee::Sensor::BundleActivator</class>
			<library>io.macchina.xbee.sensor</library>
		</activator>
		<lazyStart>false</lazyStart>
		<runLevel>610</runLevel>
		<dependency>
			<symbolicName>io.macchina.devices</symbolicName>
			<version>[1.0.0, 2.0.0)</version>
		</dependency>
		<dependency>
			<symbolicName>io.macchina.xbee</symbolicName>
			<version>[1.0.0, 2.0.0)</version>
		</dependency>
	</manifest>
	<code>
		bin/*.dll,
		bin/*.pdb,
		bin/${osName}/${osArch}/*.so,
		bin/${osName}/${osArch}/*.dylib,
	</code>
	<files>
		bundle/*
	</files>
</bundlespec>
